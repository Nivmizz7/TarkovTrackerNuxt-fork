{
  "name": "tarkov-data-mcp",
  "version": "1.0.0",
  "description": "MCP servers for Tarkov.dev and EFT Wiki",
  "type": "module",
  "scripts": {
    "build": "npm run build:tarkov-dev && npm run build:eft-wiki",
    "build:tarkov-dev": "cd mcp/tarkov-dev && npm run build",
    "build:eft-wiki": "cd mcp/eft-wiki && npm run build",
    "dev:tarkov-dev": "cd mcp/tarkov-dev && npm run dev",
    "dev:eft-wiki": "cd mcp/eft-wiki && npm run dev"
  },
  "keywords": [
    "mcp",
    "tarkov",
    "eft",
    "graphql",
    "wiki"
  ],
  "license": "MIT"
}
